{
  "version": "1.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4
  },
  "name": "backend-tools",
  "author": "Galezy Napoles Garc√≠a",
  "module": "dist/backend-tools.esm.js",
  "size-limit": [
    {
      "path": "dist/backend-tools.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/backend-tools.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.5.5",
    "@types/supertest": "^2.0.11",
    "husky": "^7.0.1",
    "size-limit": "^5.0.2",
    "supertest": "^6.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/md5": "^2.3.1",
    "@types/mysql": "^2.15.19",
    "express": "^4.17.1",
    "md5": "^2.3.0",
    "mysql": "^2.18.1"
  },
  "description": "A collection of node tools to make express server development quicker and cleaner. Contains tools for jwt generation, SQL utilities, file management, error handling and others.",
  "keywords": ["backend","tools","easy","jwt","token","mysql","sql","handler","file", "management", "utilities"],
  "homepage": "https://github.com/Galezy25/bakend-tools"
}
